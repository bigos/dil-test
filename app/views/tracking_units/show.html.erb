<h1>TractingUnits#show</h1>
<p>Find me in app/views/tracting_units/show.html.erb</p>

<p>
  <%= @tracking_unit.inspect %>
</p>


<h2>Scheduler Dates</h2>

<table>
  <thead>
    <tr>
      <td>From</td>
      <td>To</td>
      <td>Location</td>
    </tr>
  </thead>
  <tbody>
    <% @scheduler_dates.each do |tu| %>
      <tr>
        <td><%= tu.from %></td>
        <td><%= tu.to %></td>
        <td><%= tu.location %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p>
  <%= link_to 'Go Back', :back %>
</p>
